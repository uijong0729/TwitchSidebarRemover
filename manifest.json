{
  "manifest_version": 2,

  "name": "TwitchSidebarRemover",
  "version": "1.0",
  "description": "브라우저로 트위치 홈페이지를 접속 했을 때 거슬리는 좌측 바를 제거합니다.",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
  "scripts": ["background.js"],
  "persistent": false
  },

  "permissions": ["activeTab"]


}
